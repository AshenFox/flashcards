{
  "name": "flashcards",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "_dev": "nodemon server/server.js",
    "dev": "set NODE_OPTIONS=--openssl-legacy-provider && nodemon ./server/server.ts",
    "devd": "docker-compose -f docker-compose.dev.yaml up",
    "devd-rebuild": "docker-compose -f docker-compose.dev.yaml up --no-deps --build app",
    "build": "next build",
    "_start": "node server/server.js",
    "start": "node .server/server.js",
    "build:server": "tsc --p ./server/tsconfig.server.json",
    "prod": "docker-compose -f docker-compose.prod.yaml up -d",
    "prod-rebuild": "docker-compose -f docker-compose.prod.yaml up -d --no-deps --build app",
    "lint": "next lint",
    "scp-config": "scp -r ./config/ user@host:~/apps/flashcards",
    "fix-eslint": "eslint ./ --fix",
    "pretty": "prettier --write \"./**/*.{ts,tsx,js,css,scss}\"",
    "kill": "taskkill /f /im node.exe"
  },
  "dependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/cheerio": "^0.22.31",
    "@types/config": "^0.0.41",
    "@types/express": "^4.17.13",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/node": "^17.0.23",
    "@types/react": "^18.2.66",
    "@types/react-dom": "^18.2.22",
    "@types/react-redux": "^7.1.23",
    "@types/sanitize-html": "^2.6.2",
    "@types/uuid": "^8.3.4",
    "@types/web-push": "^3.3.2",
    "axios": "^1.6.7",
    "bcryptjs": "^2.4.3",
    "cheerio": "^1.0.0-rc.3",
    "clsx": "^2.1.0",
    "config": "^3.3.2",
    "easy-speech": "^2.3.1",
    "eslint-config-next": "^14.1.3",
    "express": "^4.17.1",
    "image-search-google": "^1.3.0",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^6.3.0",
    "next": "^14.1.3",
    "normalize.css": "^8.0.1",
    "npm": "^8.6.0",
    "react": "^18.2.0",
    "react-contenteditable": "^3.3.7",
    "react-dom": "^18.2.0",
    "react-loading-skeleton": "^2.1.1",
    "react-redux": "^7.2.2",
    "react-select": "^5.8.0",
    "redux": "^4.0.5",
    "redux-devtools-extension": "^2.13.8",
    "redux-thunk": "^2.3.0",
    "sanitize-html": "^2.6.1",
    "uuid": "^8.3.1",
    "web-push": "^3.4.4"
  },
  "devDependencies": {
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-simple-import-sort": "^12.1.0",
    "nodemon": "^2.0.22",
    "prettier": "3.2.5",
    "sass": "^1.29.0",
    "tsx": "^4.7.1",
    "typescript": "^4.6.3",
    "typescript-plugin-css-modules": "^5.1.0"
  }
}
