{
  "name": "flashcards",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "set NODE_OPTIONS=--openssl-legacy-provider && nodemon ./server/index.ts",
    "devd": "docker-compose -f docker-compose.dev.yaml up",
    "devd-rebuild": "docker-compose -f docker-compose.dev.yaml up --no-deps --build app",
    "build": "next build",
    "start": "node .server/server/index.js",
    "build:server": "tsc --p ./server/tsconfig.json && tsc-alias -p ./server/tsconfig.json",
    "prod": "docker-compose -f docker-compose.prod.yaml up -d",
    "prod-rebuild": "docker-compose -f docker-compose.prod.yaml up -d --no-deps --build app",
    "lint": "next lint",
    "typescript": "tsc --extendedDiagnostics --noEmit",
    "eslint": "eslint ./",
    "eslint:fix": "eslint ./ --fix",
    "stylelint:fix": "stylelint '**/*.{css,scss}' --fix",
    "pretty": "prettier --write \"./**/*.{ts,tsx,js,css,scss}\"",
    "kill": "taskkill /f /im node.exe",
    "clean-repo": "java -jar ./bfg-1.14.0.jar --replace-text passwords.txt",
    "scp-config": "scp -r ./config/ user@host:~/apps/flashcards"
  },
  "dependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/cheerio": "^0.22.31",
    "@types/config": "^0.0.41",
    "@types/express": "^4.17.13",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/node": "^17.0.23",
    "@types/react": "^18.2.66",
    "@types/react-dom": "^18.2.22",
    "@types/react-redux": "^7.1.23",
    "@types/sanitize-html": "^2.6.2",
    "@types/uuid": "^8.3.4",
    "@types/web-push": "^3.3.2",
    "axios": "^1.6.7",
    "bcryptjs": "^2.4.3",
    "cheerio": "^1.0.0-rc.3",
    "clsx": "^2.1.0",
    "config": "^3.3.2",
    "easy-speech": "^2.4.0",
    "eslint-config-next": "^14.2.3",
    "express": "^4.17.1",
    "image-search-google": "^1.3.0",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^6.3.0",
    "next": "^14.2.3",
    "next-themes": "^0.3.0",
    "normalize.css": "^8.0.1",
    "npm": "^8.6.0",
    "query-string": "^9.1.1",
    "react": "^18.3.1",
    "react-contenteditable": "^3.3.7",
    "react-dom": "^18.3.1",
    "react-loading-skeleton": "^3.4.0",
    "react-redux": "^7.2.2",
    "react-select": "^5.8.0",
    "redux": "^4.0.5",
    "redux-devtools-extension": "^2.13.8",
    "redux-thunk": "^2.3.0",
    "sanitize-html": "^2.13.0",
    "uuid": "^8.3.1",
    "web-push": "^3.4.4"
  },
  "devDependencies": {
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-simple-import-sort": "^12.1.0",
    "nodemon": "^2.0.22",
    "prettier": "3.2.5",
    "sass": "^1.29.0",
    "stylelint": "^16.5.0",
    "stylelint-config-idiomatic-order": "^10.0.0",
    "stylelint-config-standard": "^36.0.0",
    "stylelint-config-standard-scss": "^13.1.0",
    "stylelint-order": "^6.0.4",
    "tsc-alias": "^1.8.10",
    "tsx": "^4.7.1",
    "typescript-plugin-css-modules": "^5.1.0"
  }
}
